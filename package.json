{
    "name": "istrust",
    "version": "2.7.0",
    "description": "Browser extension to verify the trustability of a website",
    "homepage": "https://github.com/Internet-Society-Belgium/isTrust#readme",
    "bugs": {
        "url": "https://github.com/Internet-Society-Belgium/isTrust/issues"
    },
    "author": {
        "name": "Progiciel",
        "url": "https://progiciel.be/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Internet-Society-Belgium/isTrust.git"
    },
    "scripts": {
        "prepare": "husky install",
        "lint": "eslint --fix",
        "format": "prettier --json-recursive-sort -w .",
        "lint_format:staged": "lint-staged",
        "test": "jest",
        "serve:firefox-desktop": "webpack build --mode development --watch --env target=firefox-desktop",
        "serve:chromium": "webpack build --mode development --watch --env target=chromium",
        "build": "npm i && webpack build --mode production",
        "pack": "node utils/pack.js",
        "commit": "git pull && cz",
        "release": "dotenv release-it",
        "release:patch": "dotenv release-it patch",
        "release:minor": "dotenv release-it minor",
        "release:major": "dotenv release-it major"
    },
    "dependencies": {
        "axios": "^0.21.4",
        "psl": "^1.8.0",
        "vue": "^3.2.12",
        "vue-router": "^4.0.0"
    },
    "devDependencies": {
        "@heroicons/vue": "^1.0.4",
        "@release-it/conventional-changelog": "^3.3.0",
        "@trivago/prettier-plugin-sort-imports": "^2.0.4",
        "@types/jest": "^27.0.1",
        "@types/psl": "^1.1.0",
        "@types/webextension-polyfill": "^0.8.0",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "^5.0.0-beta.4",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/compiler-sfc": "^3.2.12",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-standard": "^6.0.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "archiver": "^5.3.0",
        "autoprefixer": "^10.3.3",
        "babel-eslint": "^10.1.0",
        "commitizen": "^4.2.4",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.2.0",
        "css-minimizer-webpack-plugin": "^3.0.2",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv-cli": "^4.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-promise": "^5.0.0",
        "eslint-plugin-vue": "^7.18.0",
        "file-loader": "^6.2.0",
        "husky": "^7.0.0",
        "jest": "^27.2.0",
        "lint-staged": "^11.1.2",
        "postcss": "^8.3.6",
        "postcss-loader": "^6.1.1",
        "prettier": "^2.4.1",
        "prettier-plugin-sort-json": "^0.0.2",
        "release-it": "^14.11.5",
        "source-map-loader": "^3.0.0",
        "style-loader": "^3.0.0",
        "tailwindcss": "^2.2.15",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.3",
        "typescript": "^4.4.3",
        "vue-eslint-parser": "^7.10.0",
        "vue-loader": "^16.0.0",
        "web-ext": "^6.4.0",
        "web-ext-plugin": "^1.3.3",
        "webextension-polyfill": "^0.8.0",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.1.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
